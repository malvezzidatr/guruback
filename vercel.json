{
    "name": "guruapi",
    "version": 2,
    "public": true,
    "build": {
        "src": "src/server.ts",
        "use": "@vercel/node"
    },
    "routes": [
        {
            "src": "src/routes/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }] 
}